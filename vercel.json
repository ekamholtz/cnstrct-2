{
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install",
  "framework": "vite",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/proxy/stripe",
      "destination": "/api/proxy/stripe.js"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
